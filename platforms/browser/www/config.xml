<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="30" id="com.sino.ttcprotekt" version="1.1.7" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>TTC Protect</name>
    <description>
        TTC Protect is a world leading vehicle location system providing protection
    </description>
    <author email="andrew@thetrackingcentre.com" href="http://www.thetrackingcentre.com">
        M2M Global Technology Limited
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <splash src="splash.png" />
    <icon src="icon.png" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="android-minSdkVersion" value="18" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="deployment-target" value="10.2" />
    <preference name="StatusBarOverlaysWebView" value="false" />
    <preference name="StatusBarStyle" value="blacktranslucent" />
    <preference name="phonegap-version" value="cli-7.1.0" />
</widget>
